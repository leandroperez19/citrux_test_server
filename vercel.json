{
    "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "PUT", "POST", "DELETE"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}